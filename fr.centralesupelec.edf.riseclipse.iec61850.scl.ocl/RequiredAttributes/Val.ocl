import scl: 'http://www.iec.ch/61850/2003/SCL'

package scl

context Val
-- extends NONE

    -- The sGroup attribute is an unsigned int
    inv Val_sGroup_valid
        ( 'sGroup attribute shall be valid in Val (line ' + self.lineNumber.toString() + '). '
          + 'Current value is ' + self.sGroup.toString()
        )
    :
        self.sGroup <> null implies self.sGroup >= 0


endpackage

