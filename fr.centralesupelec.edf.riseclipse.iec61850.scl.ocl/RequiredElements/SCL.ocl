import scl: 'http://www.iec.ch/61850/2003/SCL'

package scl

context SCL

    -- An Header is required
    inv SCL_one_Header_required
        ( 'SCL shall contain an Header (line ' + self.lineNumber.toString() + ')' )
    :
        self.Header <> null


endpackage

